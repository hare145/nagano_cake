<p>会員情報編集</p>

<%= form_with model: @customer, url: customers_information_path do |f| %>

  <h4>氏名</h4>
  <%= f.text_field :last_name %>
  <%= f.text_field :first_name %>
  
  <h4>フリガナ</h4>
  <%= f.text_field :last_name_kana %>
  <%= f.text_field :first_name_kana %>
  
  <%= f.submit '編集内容を保存' %>
  
  <%= link_to "退会する", customers_unsubscribe_path %>
  
<% end %>